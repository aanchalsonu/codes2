/*
Recently, Chef visited his doctor. The doctor advised Chef to drink at least 20002000 ml of water each day.

Chef drank XX ml of water today. Determine if Chef followed the doctor's advice or not.

*/

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long int t,n;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    if(n>=2000)
	    {
	        cout<<"YES\n";
	    }
	    else
	    {
	        cout<<"NO\n";
	    }
	}
	return 0;
}
