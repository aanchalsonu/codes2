//program to use time function 

#include<stdio.h>
#include<stdlib.h>
#include<time.h>
int main()
{
    time_t c; //time_t is the predefined structure of time and c is the structure variable 
    // time_t *p;
    time(&c);
    printf("%s\n",ctime(&c));//ctime is the function used to give the time 
    printf("Universal time is %s \n",asctime(p)); 
    printf("\nCPU starts at %ld ",clock()); //clock gives current time of system 
    printf("\nCPU ends at %ld ",clock());
}
